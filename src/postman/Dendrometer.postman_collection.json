					"raw": "{\n  \"devices\": [\n    {\n      \"device_id\": \"node_0\",\n      \"entity_name\": \"LORA-N-0\",\n      \"entity_type\": \"LoraDevice\",\n      \"timezone\": \"Europe/Madrid\",\n      \"attributes\": [\n        {\n          \"object_id\": \"analog_in_1\",\n          \"name\": \"analog_in_1\",\n          \"type\": \"Number\"\n        }\n      ],\n      \"internal_attributes\": {\n        \"lorawan\": {\n          \"application_server\": {\n            \"host\": \"eu.thethings.network\",\n            \"username\": \"{{TTN_app_id}}\",\n            \"password\": \"{{TTN_app_pw}}\",\n            \"provider\": \"TTN\"\n          },\n          \"dev_eui\": \"{{TTN_dev_eui}}\",\n          \"app_eui\": \"{{TTN_app_eui}}\",\n          \"application_id\": \"{{TTN_app_id}}\",\n          \"application_key\": \"{{TTN_app_skey}}\"\n        }\n      }\n    }\n  ]\n}",
					"options": {
						"raw": {}
					}
				},
